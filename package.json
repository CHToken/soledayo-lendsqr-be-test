{
  "dependencies": {
    "axios": "^1.13.5",
    "dotenv": "^17.3.1",
    "express": "^5.2.1",
    "jsonwebtoken": "^9.0.3",
    "knex": "^3.1.0",
    "mysql2": "^3.17.4",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/express": "^5.0.6",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^25.3.0",
    "@types/supertest": "^6.0.3",
    "@types/uuid": "^9.0.0",
    "jest": "^30.2.0",
    "supertest": "^7.2.2",
    "ts-jest": "^29.4.6",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.9.3"
  },
  "name": "lendsqr",
  "version": "1.0.0",
  "description": "A wallet service API built with NodeJS, TypeScript, KnexJS, and MySQL for the Lendsqr Backend Engineer Assessment.",
  "main": "index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/app.ts",
    "build": "tsc",
    "migrate": "knex --knexfile knexfile.ts migrate:latest --env production",
    "start": "npm run build && npm run migrate && node dist/app.js",
    "migrate:rollback": "knex --knexfile knexfile.ts migrate:rollback --env production",
    "test": "jest --runInBand --forceExit"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "testMatch": [
      "**/tests/**/*.test.ts"
    ],
    "forceExit": true
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs"
}